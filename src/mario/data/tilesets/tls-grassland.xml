<?xml version="1.0" encoding="utf-8"?>
<tileset name="grassland">

<!-- 
Tileset setup:
  0-100: Background tiles
    0: Empty tile
    1-100: background tiles (dirt etc.)

  101-200: Normal ground/wall tiles
  201-300: "Special" ground/wall tiles (like the one-way flat edge tiles)
  301-400: Decoration tiles (for instance, bushes and such)
  401-500: Pipes and such

-->

<!-- "background" tiles (non-blocking) -->
  <tile texture="gfx/tiles/empty.tga" block="0" nexttile="-1" delay="0" id="0" />
  <tile texture="gfx/tiles/dirt.tga" block="0" nexttile="-1" id="1" />

